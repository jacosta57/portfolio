{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:turbopack": "next dev --turbopack",
    "build": "next build && next-image-export-optimizer",
    "build:images": "next-image-export-optimizer",
    "start": "serve out -p 3001",
    "lint": "next lint && tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf out .next tsconfig.tsbuildinfo next-env.d.ts && npm ci",
    "clean:noinstall": "rm -rf out .next tsconfig.tsbuildinfo next-env.d.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "bootstrap": "^5.3.7",
    "husky": "^9.1.7",
    "jest": "^30.0.5",
    "next": "15.5.2",
    "next-image-export-optimizer": "^1.19.0",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-icons": "^5.5.0",
    "serve": "^14.2.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.11",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "typescript": "^5"
  },
  "description": "Jayson Acosta's Portfolio website build with NextJS and Typescript",
  "main": "src/app/layout.tsx",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JaysonJAcosta5704/portfolio.git"
  },
  "keywords": [
    "NextJS",
    "TypeScript",
    "Bootstrap"
  ],
  "author": "Jayson Acosta",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/JaysonJAcosta5704/portfolio/issues"
  },
  "homepage": "https://github.com/JaysonJAcosta5704/portfolio#readme"
}
